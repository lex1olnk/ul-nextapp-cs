{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next",
        "config": { 
          "outputDirectory": "frontend/.next"
        }
      },
      {
        "src": "api/**/*.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/.next/$1"
      }
    ]
  }