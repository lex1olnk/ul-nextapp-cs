{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "build",
          "cleanUrls": true 
        }
      },
      {
        "src": "api/**/*.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }