{
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api/main.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.go"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
  }