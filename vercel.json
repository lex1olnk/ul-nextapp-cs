{
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.go"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/api/vercel.go"
        }
      ]
  }