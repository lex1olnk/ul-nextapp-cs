{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next",
        "config": {
          "outputDirectory": ".next"
        }
      }
    ],
    "routes": [
      {
        "src": "/_next/static/(.*)",
        "dest": "frontend/.next/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/.next/server/pages/$1"
      }
    ]
  }